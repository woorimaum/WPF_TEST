<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">
    <LinearGradientBrush x:Key="LightTitleBackground"
                         StartPoint="0,0"
                         EndPoint="1,1"
                         po:Freeze="True">
        <GradientStop Offset="0"
                      Color="#FF0066FF" />
        <GradientStop Offset="1"
                      Color="#FF34D0B3" />
    </LinearGradientBrush>

    <Color x:Key="DefaultMainWinTitleButtonColor">#FFB0DCF1</Color>

    <SolidColorBrush x:Key="DefaultMainWinTitleButton" Color="{DynamicResource DefaultMainWinTitleButtonColor}" />

</ResourceDictionary>